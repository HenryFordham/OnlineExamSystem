<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
    http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <navigation-rule>
    <from-view-id>/index.xhtml</from-view-id>
    <navigation-case>
        <from-outcome>AdmOK</from-outcome>
        <to-view-id>/AdmLoginSuccess.xhtml</to-view-id>
    </navigation-case>
    <navigation-case>
        <from-outcome>AdmPassWrong</from-outcome>
        <to-view-id>/AdmLoginFaliure.xhtml.xhtml</to-view-id>
    </navigation-case>
    <navigation-case>
        <from-outcome>StuOK</from-outcome>
        <to-view-id>/StuLoginSuccess.xhtml</to-view-id>
        <redirect></redirect>
    </navigation-case>
    <navigation-case>
        <from-outcome>StuPassWrong</from-outcome>
        <to-view-id>/StuLoginFaliure.xhtml</to-view-id>
    </navigation-case>
    <navigation-case>
        <from-outcome>NoAccount</from-outcome>
        <to-view-id>/NoAccount.xhtml</to-view-id>
    </navigation-case>
        <navigation-case>
            <from-outcome>Table</from-outcome>
            <to-view-id>/showclass.xhtml</to-view-id>
        </navigation-case>
</navigation-rule>

    <navigation-rule>
        <from-view-id>/Register.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>InfoCorrect</from-outcome>
            <to-view-id>/StuRegisterSuccess.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>


    <navigation-rule>
        <from-view-id>/StuLoginSuccess.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>CouOK</from-outcome>
            <to-view-id>/showclass.xhtml</to-view-id>
            <redirect></redirect>
        </navigation-case>
        <navigation-case>
            <from-outcome>GOINSERT</from-outcome>
            <to-view-id>/insertCourse.xhtml</to-view-id>
            <redirect></redirect>
        </navigation-case>
        <navigation-case>
            <from-outcome>GOMANAGE</from-outcome>
            <to-view-id>/manageCourse.xhtml</to-view-id>
            <redirect></redirect>
        </navigation-case>
        <navigation-case>
            <from-outcome>true</from-outcome>
            <to-view-id>/showcourse.xhtml</to-view-id>
            <redirect></redirect>
        </navigation-case>
        <navigation-case>
            <from-outcome>GODELETE</from-outcome>
            <to-view-id>/manageCourse.xhtml</to-view-id>
            <redirect></redirect>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/showcourse.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>true</from-outcome>
